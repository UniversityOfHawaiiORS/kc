<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    <!-- KC-605 BEGIN - rbl need to increase the text input size by one to allow input sponsor deadline time in the following 24HR format: 23:05 p.m. azost -->
	<!-- the periods will be removed prior to insertion in the database so overall length will still fall w/in range excepted by the database field length of 15 -->
        <bean id="InstitutionalProposal-deadlineTime" parent="InstitutionalProposal-deadlineTime-parentBean">
        <property name="maxLength" value="16" />
		<property name="control">
			<bean parent="TextControlDefinition" p:size="16" />
		</property>
        </bean>
    <!-- KC-605 END -->
</beans>