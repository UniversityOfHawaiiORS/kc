        <bean parent="FieldDefinition" p:attributeName="extendedAttributes.directoryTitle" />
        <bean parent="FieldDefinition" p:attributeName="facultyFlag" />
        <bean parent="FieldDefinition" p:attributeName="researchStaffFlag" />
        <bean parent="FieldDefinition" p:attributeName="graduateStudentStaffFlag" />
        <bean parent="FieldDefinition" p:attributeName="extendedAttributes.officeLocation" />
        <bean parent="FieldDefinition" p:attributeName="extendedAttributes.school" />
        <bean parent="FieldDefinition" p:attributeName="mobilePhoneNumber" />-->       
