<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="UhPersonDelegateMaintenanceDocument" parent="KcMaintenanceDocumentEntry">
    <property name="businessObjectClass" value="edu.hawaii.kra.bo.UhPersonDelegate"/>
    <property name="maintainableClass" value="edu.hawaii.kra.bo.UhPersonDelegateMaintainableImpl"/>
    <!-- <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/> -->
    <property name="maintainableSections">
      <list>
        <ref bean="UhPersonDelegateMaintenanceDocument-EditUhPersonDelegate"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>uhPersonDelegateId</value>
      </list>
    </property>
    <property name="allowsRecordDeletion" value="true" />
    <property name="documentTypeName" value="UhPersonDelegateMaintenanceDocument"/>
    <!--  Don't really need validataion rules but if we do enable the line below and create the Rule Object referenced See  -->
    <!-- <property name="businessRulesClass" value="org.kuali.kra.rules.UhPersonDelegateMaintenanceDocumentRule"/>  -->
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="UhPersonDelegateMaintenanceDocument-EditUhPersonDelegate" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit UhPersonDelegate"/>
    <property name="title" value="Edit UH Person Delegate"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="uhPersonDelegateId" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="delegatorPerson.userName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="delegateePerson.userName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="fromDate" p:required="false" />
        <bean parent="MaintainableFieldDefinition" p:name="toDate" p:required="false" />
        <bean parent="MaintainableFieldDefinition" p:name="includeDelegator" p:required="true"/>
      </list>
    </property>
  </bean>
</beans>