<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:p="http://www.springframework.org/schema/p" 
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:lang="http://www.springframework.org/schema/lang"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
                           http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd
                           http://www.springframework.org/schema/lang
                           http://www.springframework.org/schema/lang/spring-lang-3.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
                           http://www.springframework.org/schema/util
                           http://www.springframework.org/schema/util/spring-util-3.0.xsd">

    <bean id="customModuleConfiguration" class="org.kuali.rice.krad.bo.ModuleConfiguration">
        <property name="initializeDataDictionary" value="true" />
        <property name="databaseRepositoryFilePaths">
            <list>
                <value>edu/hawaii/repository-uh.xml</value>
            </list>
        </property>
        <property name="packagePrefixes">
          <list>
            <value>edu.hawaii</value>
          </list>
        </property>  
        <property name="dataDictionaryPackages">
            <list>
                <value>classpath:edu/hawaii/datadictionary/Award.xml</value>
                <value>classpath:edu/hawaii/datadictionary/AwardAdvancedSearch.xml</value>
                <value>classpath:edu/hawaii/datadictionary/AwardDocument.xml</value>
                <value>classpath:edu/hawaii/datadictionary/DevelopmentProposal.xml</value>
                <value>classpath:edu/hawaii/datadictionary/InstitutionalProposal.xml</value>
                <value>classpath:edu/hawaii/datadictionary/InstitutionalProposalAdvancedSearch.xml</value>
                <value>classpath:edu/hawaii/datadictionary/InstitutionalProposalCostShare.xml</value>
                <value>classpath:edu/hawaii/datadictionary/InstitutionalProposalUnrecoveredFandA.xml</value>
                <value>classpath:edu/hawaii/datadictionary/KcPerson.xml</value>
                <value>classpath:edu/hawaii/datadictionary/NegotiationActivity.xml</value>
                <value>classpath:edu/hawaii/datadictionary/NegotiationDocument.xml</value>
                <value>classpath:edu/hawaii/datadictionary/NonOrganizationalRolodex.xml</value>
                <value>classpath:edu/hawaii/datadictionary/Organization.xml</value>
                <value>classpath:edu/hawaii/datadictionary/PersonDocumentEmploymentInfo.xml</value>
                <value>classpath:edu/hawaii/datadictionary/PessimisticLock.xml</value>
                <value>classpath:edu/hawaii/datadictionary/ProposalDevelopmentDocument.xml</value>
                <value>classpath:edu/hawaii/datadictionary/ProposalLog.xml</value>
                <value>classpath:edu/hawaii/datadictionary/Rolodex.xml</value>
                <value>classpath:edu/hawaii/datadictionary/Sponsor.xml</value>
                <value>classpath:edu/hawaii/datadictionary/TimeAndMoneyDocument.xml</value>
                <value>classpath:edu/hawaii/datadictionary/UhAwardExtension.xml</value>
                <value>classpath:edu/hawaii/datadictionary/UhPersonDelegate.xml</value>
                <value>classpath:edu/hawaii/datadictionary/Unit.xml</value>
                <value>classpath:edu/hawaii/datadictionary/UnitAdministrator.xml</value>
                <value>classpath:edu/hawaii/datadictionary/docs/UhPersonDelegateMaintenanceDocument.xml</value>
                <value>classpath:edu/hawaii/datadictionary/docs/UnitAdministratorMaintenanceDocument.xml</value>
             </list>
        </property>   
    </bean>

    <!-- KC-326 -->
    <bean id="uhOrganizationLookupHelperService" class="edu.hawaii.organization.service.impl.UhOrganizationLookupableHelperServiceImpl" scope="prototype"/>

    <bean id="uhOrganizationLookupable" class="org.kuali.rice.kns.lookup.KualiLookupableImpl" scope="prototype">
      <property name="lookupableHelperService">
        <ref local="uhOrganizationLookupHelperService" />
      </property>
    </bean>

    <!--UH KC-367 load UH specific resources (this used to override kc bean but they removed it in 6.0 so I added parent and class locally -->  
    <bean id="kcRiceSpringResourceLoaderConfigurer" class="org.kuali.rice.core.framework.resourceloader.RiceSpringResourceLoaderConfigurer">
      <property name="localServiceName" value="KC-Context"/>
      <property name="serviceNameSpaceURI" value="KC" />
      <property name="beanNames">
      <set>
          <value>awardAdvancedSearchLookupable</value>
          <value>institutionalProposalAdvancedSearchLookupable</value>
          <value>uhOrganizationLookupable</value>
          <value>UhPersonDelegateLookupable</value>
      </set>
      </property>
    </bean>   
    
    <!--  KC-679 New routing for 5.1.1 -->
       
    <bean id="UhPersonDelegateLookupableHelperService" class="edu.hawaii.kra.service.impl.UhPersonDelegateLookupableHelperServiceImpl" scope="prototype"/>

    <bean id="UhPersonDelegateLookupable" class="org.kuali.rice.kns.lookup.KualiLookupableImpl" scope="prototype">
      <property name="lookupableHelperService">
        <ref local="UhPersonDelegateLookupableHelperService" />
      </property>
    </bean>

    <!-- KC-809 eDocLite doesn't work in KC when running bundled mode -->
    <bean id="edlConfigurer" class="org.kuali.rice.edl.impl.config.EDLConfigurer"/>      
</beans>
