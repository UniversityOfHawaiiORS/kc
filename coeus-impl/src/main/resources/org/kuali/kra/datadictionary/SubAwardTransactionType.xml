<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="SubAwardTransactionType" parent="SubAwardTransactionType-parentBean" />

  <bean id="SubAwardTransactionType-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.kra.subaward.bo.SubAwardTransactionType" />
    <property name="objectLabel" value="Subaward Transaction Type"/>
    <property name="lookupDefinition" >
      <ref bean="SubAwardTransactionType-lookupDefinition" />
    </property>
	<property name="attributes" >
      <list>
      	<ref bean="SubAwardTransactionType-transactionTypeCode" />
        <ref bean="SubAwardTransactionType-transactionTypeDescription" />
      </list>
    </property>
  </bean>

<!-- Attribute Definitions -->

  <bean id="SubAwardTransactionType-transactionTypeCode" parent="SubAwardTransactionType-transactionTypeCode-parentBean" />
  <bean id="SubAwardTransactionType-transactionTypeCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionTypeCode" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Transaction Type Code" />
    <property name="shortLabel" value="Transaction Type Code" />
    <property name="maxLength" value="60" />
     <property name="validationPattern" >
      <bean parent="AlphaNumericValidationPattern" />
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="60" />
    </property>
    <property name="summary" value="TransactionTypeCode" />
    <property name="description" value="TransactionTypeCode" />
  </bean>
    
 
 <bean id="SubAwardTransactionType-transactionTypeDescription" parent="SubAwardTransactionType-transactionTypeDescription-parentBean" />
  <bean id="SubAwardTransactionType-transactionTypeDescription-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="transactionTypeDescription" />
    <property name="forceUppercase" value="false" />
    <property name="label" value="Transaction Type Description" />
    <property name="shortLabel" value="Transaction Type Description" />
    <property name="maxLength" value="60" />
     <property name="validationPattern" >
      <bean p:allowWhitespace="true" parent="AnyCharacterValidationPattern"/>
    </property>
    <property name="required" value="true" />
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="60" />
    </property>
    <property name="summary" value="TransactionTypeDescription" />
    <property name="description" value="TransactionTypeDescription" />
  </bean>
  
   <bean id="SubAwardTransactionType-lookupDefinition" parent="SubAwardTransactionType-lookupDefinition-parentBean" />

  <bean id="SubAwardTransactionType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Subaward Transaction Type Lookup" /><property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean p:attributeName="transactionTypeCode" parent="FieldDefinition" p:forceInquiry="true" />
        <bean p:attributeName="transactionTypeDescription" parent="FieldDefinition" p:forceInquiry="true" />
               
        
        
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean p:attributeName="transactionTypeCode" parent="FieldDefinition" p:forceInquiry="true"/>
        <bean p:attributeName="transactionTypeDescription" parent="FieldDefinition" p:forceInquiry="true" />
        
        
      </list>
    </property>
  </bean>
	
  
 
</beans>
